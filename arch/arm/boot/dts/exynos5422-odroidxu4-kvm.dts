#include "exynos5422-odroidxu4.dts"

/ {
	timer {
		compatible = "arm,cortex-a15-timer",
			   "arm,armv7-timer";
		interrupts = <1 13 0xf08>,
			   <1 14 0xf08>,
			   <1 11 0xf08>,
			   <1 10 0xf08>;
		clock-frequency = <24000000>;
	};
};
